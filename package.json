{
  "name": "koa-router-zod-swagger",
  "version": "2.0.1",
  "description": "Generate swagger specs and host ui based on koa-router and zod configuration",
  "keywords": [
    "koa",
    "router",
    "zod",
    "swagger"
  ],
  "homepage": "https://github.com/vaso991/koa-router-zod-swagger#readme",
  "author": "vaso991 (https://github.com/vaso991/)",
  "repository": {
    "url": "https://github.com/vaso991/koa-router-zod-swagger"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "watch:ts": "tsc -w",
    "watch:example": "node --watch --experimental-strip-types example/index.ts",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "koa": "^2.14.2",
    "koa-json-error": "^3.1.2",
    "koa-router": "^12.0.0",
    "zod": "^4.0.0"
  },
  "dependencies": {
    "koa2-swagger-ui": "^5.11.0",
    "statuses": "^2.0.2"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^22.13.10",
    "@types/statuses": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "koa-body": "^6.0.1"
  }
}
