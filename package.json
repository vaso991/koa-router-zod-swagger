    "clean": "rimraf dist",
    "prepack": "npm run build",
    "test": "eslint ."
  },
  "peerDependencies": {
    "koa": "^2.14.2",
    "koa-json-error": "^3.1.2",
    "koa-router": "^12.0.0"
