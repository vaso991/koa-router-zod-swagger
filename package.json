  },
  "devDependencies": {
    "@types/koa": "^2.13.6",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^20.3.2",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "rimraf": "^4.1.2",
    "typescript": "^5.0.4"
  },
  "optionalDependencies": {
    "koa-body": "^6.0.1"
  }
